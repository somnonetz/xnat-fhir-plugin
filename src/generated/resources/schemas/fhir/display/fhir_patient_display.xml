	<ViewLink alias="FHIR_PATIENT_PROJECTS">
		<Mapping TableName="FHIR_PATIENT_PROJECTS">
			<MappingColumn rootElement="fhir:patient" fieldElement="fhir:patient.ID" mapsTo="id"/>
		</Mapping>
	</ViewLink>
	<SQLView name="FHIR_PATIENT_PROJECTS" sql="SELECT id, '&lt;' || expt.project || '&gt;' || xs_a_concat(',&lt;' || shared.project || '&gt;') AS projects FROM xnat_experimentData expt LEFT JOIN xnat_experimentData_share shared ON expt.id=shared.sharing_share_xnat_experimentda_id LEFT JOIN xdat_meta_element xme ON expt.extension = xme.xdat_meta_element_id WHERE element_name='fhir:patient' GROUP BY expt.id,expt.project"/>
