	</DisplayField>
	<DisplayField id="MULTIPLEBIRTH_MULTIPLEBIRTHINTEGER" header="multipleBirthInteger" visible="true" searchable="true">
		<DisplayFieldElement name="Field1" schema-element="fhir:patient/multipleBirth/multipleBirthInteger"/>
	</DisplayField>
	<DisplayField id="ANIMAL_SPECIES" header="species" visible="true" searchable="true">
		<DisplayFieldElement name="Field1" schema-element="fhir:patient/animal/species"/>
	</DisplayField>
	<DisplayField id="ANIMAL_BREED" header="breed" visible="true" searchable="true">
		<DisplayFieldElement name="Field1" schema-element="fhir:patient/animal/breed"/>
	</DisplayField>
	<DisplayField id="ANIMAL_GENDERSTATUS" header="genderStatus" visible="true" searchable="true">
		<DisplayFieldElement name="Field1" schema-element="fhir:patient/animal/genderStatus"/>
	</DisplayField>
	<DisplayField id="MANAGINGORGANIZATION_ID" header="managingOrganization_ID" visible="true" searchable="true">
		<DisplayFieldElement name="Field1" schema-element="fhir:patient/managingOrganization_ID"/>
	</DisplayField>
